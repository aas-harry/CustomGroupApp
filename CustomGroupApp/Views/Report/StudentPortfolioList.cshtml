<div id="student-portfolio-list-container">
    <table>
        <tr>
            <td>
                <div id="student-list-container"></div>
            </td>
            <td>
                <div>
                    <button class="btn btn-default" onClick="schoolStudentRecord()">School Student Record</button>
                    <button class="btn btn-default" onClick="studentNaplanReport()">Student Naplan</button>
                </div>

                <div id="student-report" style="vertical-align: top">
                    <p>Select a report</p>
                </div>
            </td>
        </tr>
    </table>
</div>

<script>

    var studentPortfolioViewModel;
    $(document).ready(function () {
        var studentListControl = new StudentListControl("student-list-container");
        studentListControl.create("Students",
            function (student) {
                studentPortfolioViewModel.showStudentReport(student);

            },
            300, 600);

        studentPortfolioViewModel = new StudentPortfolioViewModel();
        studentPortfolioViewModel.setDatasource(testFile);
        studentListControl.setDatasource(testFile.students);

    });

    function schoolStudentRecord() {
        studentPortfolioViewModel.setReport(studentPortfolioViewModel.studentReports[0]);
    }

    function studentNaplanReport() {
       
        studentPortfolioViewModel.setReport(studentPortfolioViewModel.studentReports[1]);
    }
</script>